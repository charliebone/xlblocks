<?xml version="1.0" encoding="utf-8" ?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load" loadImage="Ribbon_LoadImage">
  <ribbon>
    <tabs>
      <tab id="tab_XlBlocks" label="XlBlocks">
        <group id="group_cache" label="Cache">
          <button id="button_viewcachedobject" label="View Object" keytip="U" imageMso="AdpDiagramTableModesMenu" size="large" description="View cached object" onAction="Ribbon_OnButtonPressed"/>
          <button id="button_viewcache" label="View Cache"  keytip="C" imageMso="RecordsInsertSubdatasheet" size="normal" description="Open cache viewer" onAction="Ribbon_OnButtonPressed"/>
          <button id="button_clearcache" label="Delete Cache"  keytip="X" imageMso="SheetDelete" size="normal" description="Clear cache" onAction="Ribbon_OnButtonPressed"/>
        </group>
        <group id="group_debug" label="Debug">
          <button id="button_erroroutput" getLabel="Ribbon_GetLabel" keytip="E" getImage="Ribbon_GetImage" size="large" description="Turn error output on/off" onAction="Ribbon_OnButtonPressed"/>
          <button id="button_errorlog" label="Error Log" keytip="L" imageMso="FieldCodes" size="normal" description="Open error viewer" onAction="Ribbon_OnButtonPressed"/>
        </group>
        <group id="group_info" label="About">
          <button id="button_about" label="About" keytip="A" image="xlblocks_logo_square" size="large" description="Display information about the XlBlocks AddIn" onAction="Ribbon_OnButtonPressed"/>
        </group>
      </tab>
    </tabs>	
  </ribbon>
</customUI>